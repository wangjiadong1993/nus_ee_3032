#include "SD_3032.h"
#include "Temp_3032.h"

volatile uint32_t Timer = 0;
void SysTick_Handler (void) /* SysTick Interrupt Handler (1ms)   */
{
    static uint32_t ticks;

    /* LED blink at frequency 1Hz */
    if (ticks++ >=10)
    {
        disk_timerproc();
        ticks = 0;
    }

    Timer++;

}
//finished time handler
int initialization_3032()
{
	//GPS	//GPS switch
	init_uart2();
	//SD card

	//load sensor & heater
	//temp sensor
	//GSM
	//Bluetooth
}



int main()
{

	return 0 ;
}
