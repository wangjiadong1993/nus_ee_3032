
int main(void) {
	int adval;
	int low = 0;
	init_temp();
	init_gpio_2_8();
	if (SysTick_Config(SystemCoreClock / 1000)) {
		while (1);  // Capture error
	}
	while(1) {

		adval=read_temp();
		printf("%d\n", adval);
		if(adval>2300)
		{
			GPIO_SetValue(2, 1<<8);
		}else if(adval<2250)
		{
			GPIO_ClearValue(2, 1<<8);
		}
		systick_delay(200);
		}
	return 0 ;
}
